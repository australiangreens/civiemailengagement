<?xml version="1.0"?>
<extension key="civiemailengagement" type="module">
  <file>civiemailengagement</file>
  <name>CiviEmailEngagement</name>
  <description>Email engagement model for CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>John Twyman</author>
    <email>john.twyman@greens.org.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/australiangreens/civiemailengagement</url>
    <url desc="Documentation">https://github.com/australiangreens/civiemailengagement</url>
    <url desc="Support">https://github.com/australiangreens/civiemailengagement</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-12-05</releaseDate>
  <version>2.1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.70</ver>
  </compatibility>
  <requires>
    <ext>civimodels</ext>
  </requires>
  <comments>This extension adds an email engagement model for analytics</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/CiviEmailEngagement</namespace>
    <format>24.09.1</format>
    <angularModule>crmCiviemailengagement</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\Civiemailengagement\AutomaticUpgrader</upgrader>
</extension>
